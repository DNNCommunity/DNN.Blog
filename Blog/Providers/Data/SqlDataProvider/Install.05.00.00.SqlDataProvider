AS 
 BEGIN
  SELECT  *
  FROM    {objectQualifier}Blog_Tags
  ORDER BY PortalID ASC ,
    Tag ASC
 END
GO

IF  EXISTS (SELECT * FROM sys.objects WHERE object_id = OBJECT_ID(N'{databaseOwner}{objectQualifier}Blog_Upgrade_UpdateModuleDefId') AND type in (N'P', N'PC'))
