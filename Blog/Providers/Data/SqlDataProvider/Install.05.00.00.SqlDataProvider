   ) AS ChildBlogCount ,
   ( SELECT    COUNT(BlogID)
     FROM      {objectQualifier}Blog_Entries
     WHERE     BlogID = B.BlogID
      AND Published = 1
   ) AS BlogPostCount
 FROM    {objectQualifier}Blog_Blogs B
